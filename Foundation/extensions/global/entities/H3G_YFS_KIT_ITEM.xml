		        Extensible="Y"
		        Module="ycm"
		        Name="Kit_Item"
		        Prefix="YFS_"
		        TableName="YFS_KIT_ITEM"
		        TableType="MASTER"
		        XMLName="KitItem"
		        LockingEntity="Item">
