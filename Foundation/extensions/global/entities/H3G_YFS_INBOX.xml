<DBSchema>
	<Entities>   
		<Entity TableName="YFS_INBOX">
			<Attributes>
				<Attribute ColumnName="EXTN_INCIDENT_NO"
			           	DataType="Text-40"
			           	Description="This attribute stores the Incident no coming from BPM"
						Type="CHAR"
			           	XMLName="ExtnIncidentNo"
			           	XMLGroup="Extn"
			           	Nullable="true"/>
			     <Attribute ColumnName="EXTN_RAISE_TICKET"
			           	DataType="Flag"
			           	Description="This attribute checks if trouble ticket is to be raised"
						Type="CHAR"
			           	XMLName="ExtnRaiseTicket"
			           	XMLGroup="Extn"
			           	Nullable="false"
			           	DefaultValue="Y"/>
			</Attributes>
		</Entity>
	</Entities>
</DBSchema>

		
		
		

	