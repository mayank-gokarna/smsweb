<?xml version="1.0" encoding="UTF-8"?>
<ExtensionMetaData>
    <ScreenExtension extnClassName="OrderSearchExtn"
        extnPackage="extn.order.search"
        originalScreen="isccs.order.search.OrderSearch"
        originalScreenPackage="isccs.order.search" type="diffrential">
        <Items>
            <RadioButtonSet _sc_extensibility_moveOrder="2"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="before"
                _sc_extensibility_referenceUId="extn_radCustomerOrder"
                groupAlignment="horizontal"
                label="$(_scSimpleBundle:OrderStatus)"
                scuiDataType="Text-20" uId="extn_radOrderStatus">
                <BindingData>
                    <RadioBinding>
                        <Option label="$(_scSimpleBundle:extn_Rejected)"
                            scExtensibilityArrayItemId="extn_RadioBinding_0" value="1100.020"/>
                        <Option
                            label="$(_scSimpleBundle:extn_Suspended)"
                            scExtensibilityArrayItemId="extn_RadioBinding_1" value="1100.050"/>
                        <Option label="$(_scSimpleBundle:extn_All)"
                            scExtensibilityArrayItemId="extn_RadioBinding_2"
                            selected="true" value="All"/>
                        <Option
                            label="$(_scSimpleBundle:extn_Awaiting_Error_Resolution)"
                            scExtensibilityArrayItemId="extn_RadioBinding_3" value="1100.060"/>
                    </RadioBinding>
                    <TargetBindings>
                        <TargetBinding allowEmpty="true"
                            namespace="getAdvancedOrderList_input"
                            path="Order.Status" scExtensibilityArrayItemId="extn_TargetBindings_0"/>
                    </TargetBindings>
                </BindingData>
            </RadioButtonSet>
            <RadioButtonSet _sc_extensibility_moveOrder="1"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="before"
                _sc_extensibility_referenceUId="radReadFromHistory"
                groupAlignment="horizontal"
                label="$(_scSimpleBundle:extn_Order_Type)"
                name="OrderType" uId="extn_radCustomerOrder">
                <BindingData>
                    <RadioBinding>
                        <Option
                            label="$(_scSimpleBundle:extn_Customer_Order)"
                            scExtensibilityArrayItemId="extn_RadioBinding_0"
                            selected="true" value="CustomerOrder"/>
                        <Option label="$(_scSimpleBundle:extn_All)"
                            scExtensibilityArrayItemId="extn_RadioBinding_1" value="All"/>
                    </RadioBinding>
                    <SourceBinding namespace="screenInput" path="Order.OrderType"/>
                    <TargetBindings>
                        <TargetBinding
                            namespace="getAdvancedOrderList_input"
                            path="Order.OrderType" scExtensibilityArrayItemId="extn_TargetBindings_0"/>
                    </TargetBindings>
                </BindingData>
            </RadioButtonSet>
            <RadioButtonSet _sc_extensibility_moveOrder="3"
                _sc_extensibility_operation="remove"
                _sc_extensibility_position="first"
                _sc_extensibility_referenceUId="_extensibilityHiddenWidgetsHolder" uId="radDraftOrderFlag"/>
            <TextField _sc_extensibility_moveOrder="3"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="last"
                _sc_extensibility_referenceUId="pnlCustFields"
                label="$(_scBundle:CustomerEmailStartsWith)"
                scuiDataType="EMailID" uId="extn_txt_custEmailId">
                <BindingData>
                    <TargetBindings>
                        <TargetBinding
                            namespace="getAdvancedOrderList_input"
                            path="Order.PersonInfoBillTo.EmailID"
                            queryType="FLIKE" scExtensibilityArrayItemId="extn_TargetBindings_0"/>
                    </TargetBindings>
                    <SourceBinding namespace="screenInput" path="Order.PersonInfoBillTo.EmailID"/>
                </BindingData>
            </TextField>
            <TextField _sc_extensibility_moveOrder="9"
                _sc_extensibility_operation="move"
                _sc_extensibility_position="before"
                _sc_extensibility_referenceUId="extn_txt_custPhone" uId="extn_txt_custEmailId"/>
            <TextField _sc_extensibility_moveOrder="5"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="after"
                _sc_extensibility_referenceUId="extn_txt_custEmailId"
                label="$(_scBundle:CustomerPhoneNo)"
                scuiDataType="PhoneNo" selectOnClick="true" uId="extn_txt_custPhone">
                <BindingData>
                    <TargetBindings>
                        <TargetBinding
                            namespace="getAdvancedOrderList_input"
                            path="Order.PersonInfoBillTo.DayPhone" scExtensibilityArrayItemId="extn_TargetBindings_0"/>
                    </TargetBindings>
                    <SourceBinding namespace="screenInput" path="Order.PersonInfoBillTo.DayPhone"/>
                </BindingData>
            </TextField>
            <TextField _sc_extensibility_moveOrder="8"
                _sc_extensibility_operation="move"
                _sc_extensibility_position="before"
                _sc_extensibility_referenceUId="txt_fName" uId="extn_txt_custPhone"/>
            <TextField _sc_extensibility_moveOrder="7"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="after"
                _sc_extensibility_referenceUId="txt_lName"
                label="$(_scBundle:ZipCode)" uId="extn_txt_postalCode">
                <BindingData>
                    <TargetBindings>
                        <TargetBinding
                            namespace="getAdvancedOrderList_input"
                            path="Order.PersonInfoBillTo.ZipCode" scExtensibilityArrayItemId="extn_TargetBindings_0"/>
                    </TargetBindings>
                    <SourceBinding namespace="screenInput" path="Order.PersonInfoBillTo.ZipCode"/>
                </BindingData>
            </TextField>
            <TextField _sc_extensibility_moveOrder="4"
                _sc_extensibility_operation="remove"
                _sc_extensibility_position="last"
                _sc_extensibility_referenceUId="_extensibilityHiddenWidgetsHolder" uId="txt_custEmailId"/>
            <TextField _sc_extensibility_moveOrder="6"
                _sc_extensibility_operation="remove"
                _sc_extensibility_position="last"
                _sc_extensibility_referenceUId="_extensibilityHiddenWidgetsHolder" uId="txt_custPhone"/>
            <TextField _sc_extensibility_moveOrder="8"
                _sc_extensibility_operation="remove"
                _sc_extensibility_position="last"
                _sc_extensibility_referenceUId="_extensibilityHiddenWidgetsHolder" uId="txt_postalCode"/>
        </Items>
        <Subscribers>
            <Local>
                <Subscriber eventId="beforeBehaviorMashupCall"
                    isExtn="true" sequence="51">
                    <Handler methodName="customBeforeBehaviourMashup"/>
                </Subscriber>
                <Subscriber eventId="extn_txt_custEmailId_onKeyUp"
                    isExtn="true" sequence="51">
                    <Handler methodName="SST_invokeApiOnEnter"/>
                </Subscriber>
                <Subscriber eventId="extn_txt_custPhone_onKeyUp"
                    isExtn="true" sequence="51">
                    <Handler methodName="SST_invokeApiOnEnter"/>
                </Subscriber>
                <Subscriber eventId="extn_txt_postalCode_onKeyUp"
                    isExtn="true" sequence="51">
                    <Handler methodName="SST_invokeApiOnEnter"/>
                </Subscriber>
            </Local>
        </Subscribers>
    </ScreenExtension>
</ExtensionMetaData>
